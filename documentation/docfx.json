{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "**.csproj"
                    ],
                    "src": "../variant"
                }
            ],
            "dest": "api",
            "disableGitFeatures": false,
            "disableDefaultFilter": false
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "api/**.yml"
                ]
            },
            {
                "files": [
                    "wiki/**.md",
                    "wiki/**/toc.yml",
                    "toc.yml",
                    "*.md"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ]
            }
        ],
        "overwrite": [
            {
                "files": [
                    "apidoc/**.md"
                ],
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            }
        ],
        "dest": "_site",
        "globalMetadataFiles": [],
        "globalMetadata": {
            "_enableSearch": true,
            "_disableContribution": true
        },
        "fileMetadataFiles": [],
        "template": [
            "default"
        ],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}